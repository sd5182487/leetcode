def fourSum(self, num, target):
    num.sort()
    def ksum(num, k, target):
        i = 0
        if k == 2:
            j = len(num) - 1
            while i < j:
                if num[i] + num[j] == target:
                    yield (num[i], num[j])
                    i += 1
                elif num[i] + num[j] > target:
                    j -= 1
                else:
                    i += 1
        else:
            while i < len(num) - k + 1:
                newtarget = target - num[i]
                for sub in ksum(num[i+1:], k - 1, newtarget):
                    yield (num[i],) + sub
                i += 1

    return [list(t) for t in set(ksum(num, 4, target))]